            crossorigin="anonymous"></script>
        <script>

            // Formdaki verileri alın

            var getUser = localStorage.getItem('userData');
            var getResults = localStorage.getItem('results');
            var userObject = JSON.parse(getUser);
            var resultObject = JSON.parse(getResults);


            var names = document.getElementById("name");
            var degrees = document.getElementById("degree");
            var score = document.getElementById("score");
            var times = document.getElementById("time");

            names.value = userObject.name;
            degrees.value = userObject.degree;
            score.value = resultObject.scoreIndex;
            times.value = resultObject.timerValue;

            var intScoreIndex = Number(resultObject.scoreIndex);

            var result = document.getElementById("result");
            if (intScoreIndex >= 90) {
                result.innerHTML = "A | Excellent";
            } else if (intScoreIndex >= 70) {
                result.innerHTML = "B | Good";
            } else if (intScoreIndex >= 40) {
                result.innerHTML = "C | Bad";
            } else {
                result.innerHTML = "D | Try Again";
            }



            var backButton = document.getElementById("backButton");
            backButton.addEventListener("click", function () {

                var targetURL = "/";

                window.location.href = targetURL;
            });
        </script>

